{
"manifest_version": 2,
  "name": "ScriptparserExtension",
  "version": "1.0",
  "description": "Ladet die Scripte erneut aus der besuchten Website und die können über den Popup eingesehen werden",
  "icons": { "48": "zorro.jpg" },

    "browser_action": {
    "default_icon": "zorro.jpg",
    "default_popup": "ScriptparserPopup.html"
  },
    
  "content_scripts": [
    {
    "run_at": "document_start",
    "matches": ["<all_urls>"],
    "js": ["bundle.js", "loadScripts.js", "NodeWalk.js", "bundle2.js"]
    }
  ],
   
    
  "permissions": [
    "webRequest", 
    "webRequestBlocking",
    "<all_urls>",
    "storage",
    "activeTab",
	"notifications"
  ]

}