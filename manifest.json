{
"manifest_version": 2,
  "name": "ScriptparserExtension",
  "version": "1.0",
  "description": "Ladet die Scripte erneut aus der besuchten Website und die können über den Popup eingesehen werden",
  "icons": { "48": "zorro.jpg" },
  
 	

  "browser_specific_settings": {
    "gecko": {
      "id": "addon@example.com",
      "strict_min_version": "42.0"
    }
  },

    "browser_action": {
    "default_icon": "zorro.jpg",
    "default_title": "Popup",
    "default_popup": "ScriptparserPopup.html"
  },

  "content_scripts": [
    {
    "run_at": "document_start",
    "matches": ["<all_urls>"],
    "js": ["bundle2.js", "GenObject.js", "loadScripts.js", "Regel1.js", "Regel2.js", "Regel3.js", "Regel4.js", "Regel5.js", "Regel6.js", "Regel7.js",  "NodeWalk.js"]
    }
  ],

   
    
  "permissions": [
    "webRequest", 
    "webRequestBlocking",
    "<all_urls>",
    "storage",
    "activeTab",
    "tabs",
	  "notifications",
    "contextMenus"
  ]

}